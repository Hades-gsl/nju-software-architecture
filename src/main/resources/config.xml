    <bean id="cp437_10x10" class="asciiPanel.AsciiFont">
        <constructor-arg value="cp437_10x10.png"/>
        <constructor-arg value="10"/>
        <constructor-arg value="10"/>
    </bean>
    <bean id="cp437_12x12" class="asciiPanel.AsciiFont">
        <constructor-arg value="cp437_12x12.png"/>
        <constructor-arg value="12"/>
        <constructor-arg value="12"/>
    </bean>
    <bean id="cp437_16x16" class="asciiPanel.AsciiFont">
        <constructor-arg value="cp437_16x16.png"/>
        <constructor-arg value="16"/>
        <constructor-arg value="16"/>
    </bean>
    <bean id="cp437_9x16" class="asciiPanel.AsciiFont">
        <constructor-arg value="cp437_9x16.png"/>
