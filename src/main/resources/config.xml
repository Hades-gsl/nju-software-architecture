        <constructor-arg value="10"/>
    </bean>
    <bean id="talryth_15x15" class="asciiPanel.AsciiFont">
        <constructor-arg value="talryth_square_15x15.png"/>
        <constructor-arg value="15"/>
        <constructor-arg value="15"/>
    </bean>

    <bean id="ascllPanel-talryth_15x15" class="asciiPanel.AsciiPanel">
        <constructor-arg value="10"/>
        <constructor-arg value="10"/>
        <constructor-arg ref="talryth_15x15"/>
    </bean>
    <bean id="ascllPanel-cp437_8x8" class="asciiPanel.AsciiPanel">
        <constructor-arg value="8"/>
        <constructor-arg value="8"/>
        <constructor-arg ref="cp437_8x8"/>
